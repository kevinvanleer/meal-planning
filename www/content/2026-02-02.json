  "recipes": [
    {
      "day": "Monday",
      "name": "Crockpot Crack Chicken over Rice",
      "ingredients": [
        "1 lb boneless skinless chicken thighs",
        "½ oz ranch seasoning (half packet)",
        "8 oz cream cheese",
        "4 slices bacon, cooked and chopped",
        "¾ cup shredded cheddar",
        "¼ cup sliced green onions",
        "1 cup white rice"
      ],
      "instructions": "Slow cooker LOW 7 hrs. Shred, stir, top with cheddar, melt 10 min. Serve over rice."
    },
    {
      "day": "Tuesday",
      "name": "Chicken Burrito Bowls",
      "ingredients": [
        "1 lb chicken breast, diced",
        "1 Tbsp olive oil, 1 tsp cumin, 1 tsp chili powder, ½ tsp garlic powder, salt & pepper",
        "1 cup rice",
        "1 can (15 oz) black beans, drained",
        "½ cup corn (frozen or canned)",
        "½ cup salsa, ¼ cup sour cream",
        "Toppings: shredded cheese, avocado, cilantro"
      ],
      "instructions": "Season and sauté chicken 6–7 min. Serve over rice with beans, corn, and toppings."
    },
    {
      "day": "Wednesday",
      "name": "GF Fettuccine Alfredo with Broccoli",
      "ingredients": [
        "9 oz GF fettuccine",
        "2 Tbsp butter, 1 cup heavy cream, 1 clove garlic minced",
        "¾ cup grated Parmesan, salt & pepper",
        "2 cups broccoli florets"
      ],
      "instructions": "Melt butter, sauté garlic 30 sec, add cream, simmer 3 min. Stir in Parmesan until smooth. Toss with cooked GF pasta. Steam or roast broccoli on the side."
    },
    {
      "day": "Thursday",
      "name": "Honey Garlic Salmon with Steamed Green Beans",
      "ingredients": [
        "3 salmon fillets (~5 oz each)",
        "2 Tbsp honey, 1 Tbsp tamari/GF soy sauce, 1 tsp minced garlic, 1 Tbsp butter",
        "12 oz green beans, salt & pepper"
      ],
      "instructions": "Sear salmon skin-side up 3 min, flip, add honey-garlic-tamari sauce, cook 3 more min basting. Steam green beans on the side."
    },
    {
      "day": "Friday",
      "name": "Crockpot Chicken Tortilla Soup",
      "ingredients": [
        "1 lb chicken breast",
        "1 can (14 oz) diced tomatoes, 1 can (15 oz) black beans (drained), 1 cup corn",
        "1 cup chicken broth, 1 tsp cumin, 1 tsp chili powder, ½ tsp garlic powder, salt",
        "Toppings: corn tortilla chips, shredded cheese, sour cream, avocado, cilantro, lime"
      ],
      "instructions": "Place chicken, tomatoes, beans, corn, broth, and spices in slow cooker. Cook LOW 6–8 hrs. Shred chicken, stir. Serve with toppings."
    },
    {
      "day": "Saturday",
      "name": "Crockpot Salsa Verde Chicken Tacos",
      "ingredients": [
        "1 lb boneless skinless chicken breast",
        "1 cup salsa verde",
        "1 tsp cumin, ½ tsp garlic powder, salt",
        "Corn tortillas (GF)",
        "Toppings: diced onion, cilantro, lime, shredded cheese, sour cream"
      ],
      "instructions": "Place chicken in slow cooker, pour salsa verde, season. Cook LOW 6 hrs. Shred and serve in corn tortillas."
    },
    {
      "day": "Sunday",
      "name": "Teriyaki Chicken Rice Bowls",
      "ingredients": [
        "1 lb chicken thighs, sliced",
        "Sauce: 3 Tbsp GF soy sauce/tamari, 1 Tbsp honey, 1 tsp sesame oil, 1 tsp rice vinegar, 1 tsp cornstarch + 1 Tbsp water",
        "1 cup rice",
        "1 cup steamed broccoli or snap peas"
      ],
      "instructions": "Sauté chicken 6 min, add sauce, cook until thickened. Serve over rice with veggies. Top with sesame seeds and green onion."
    }
  ],
